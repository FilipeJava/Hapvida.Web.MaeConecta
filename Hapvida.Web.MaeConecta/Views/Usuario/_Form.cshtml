@model Usuario


<div class="mb-3">
    <label asp-for="Nome"></label>
    <input asp-for="Nome" class="form-control" />
</div>
<div class="mb-3">
    <label asp-for="Idade"></label>
    <input asp-for="Idade" class="form-control" />
</div>
<div class="mb-3">
    <label asp-for="SemanaGestacao"></label>
    <input asp-for="SemanaGestacao" class="form-control" />
</div>
<div class="mb-3">
    <label asp-for="TipoSanguineo"></label>
    <select asp-for="TipoSanguineo" class="form-select"
            asp-items="@Html.GetEnumSelectList<TipoSanguineo>()">
        <option>Selecione</option>
    </select>
</div>
<div class="mb-3">
    <label asp-for="Endereco.Logradouro"></label>
    <input asp-for="Endereco.Logradouro" class="form-control" />
</div>
<div class="mb-3">
    <label asp-for="Endereco.Cep"></label>
    <input asp-for="Endereco.Cep" class="form-control" />
</div>

<div class="mb-3">
    <label asp-for="Email"></label>
    <input asp-for="Email" class="form-control" />
</div>

<div class="mb-3">
    <label asp-for="Senha"></label>
    <input asp-for="Senha" class="form-control" />
</div>

<div class="mb-3">
    <label asp-for="DataCadastro"></label>
    <input asp-for="DataCadastro" class="form-control" />
</div>
